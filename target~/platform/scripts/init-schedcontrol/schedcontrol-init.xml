<project name="schedcontrol-init">

	<import file="${scripts.dir}/extension-init.xml" />

	<target name="init/schedcontrol" extensionOf="init/execution" if="execSOAR">
		<augment id="init.soar.bootclasspath">
			<fileset dir="${microjvm.dir}/javaLibs" includes="schedcontrol-*.jar" />
		</augment>
		
		<augment id="init.requires">
			<string value="com.is2t.schedcontrol.ScheduleController"/>
		</augment>
	</target>
	
</project>